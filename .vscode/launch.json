    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Webapp",
            "type": "python",
            "request": "launch",
            "module": "flask",
            "env": {
                "FLASK_APP": "spartid_ais",
                "FLASK_DEBUG": "1",
                "SQLALCHEMY_DATABASE_URI": "postgresql://postgres:postgres@localhost:5432/spartid_ais"
            },
            "args": [
                "run",
                "--no-debugger",
                "--no-reload"
            ],
            "jinja": true,
            "justMyCode": true
        },
        {
            "name": "Python: AIS Poller",
            "type": "python",
            "request": "launch",
            "program": "spartid_ais/kystverket.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "env": {
                "SQLALCHEMY_DATABASE_URI": "postgresql://postgres:postgres@localhost:5432/spartid_ais"
            }

        }
    ]
}
